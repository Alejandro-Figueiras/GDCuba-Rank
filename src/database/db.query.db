CREATE TABLE users (
    id SERIAL PRIMARY KEY NOT NULL,
    username VARCHAR(15) NOT NULL UNIQUE,
    accountID INTEGER UNIQUE,
	password VARCHAR(30) ,
	status VARCHAR(1) NOT NULL DEFAULT 'u'
);


